                        <figure class="image-anime">
                            <img src="/images/contact-info-2.jpg" alt="">
                        </figure>
                    </div>
                </div>
            </div>

            <div class="col-md-4">
                <div class="contact-item wow fadeInUp" data-wow-delay="0.5s">
                    <div class="contact-content">
                        <div class="contact-content-title">
                            <h2>email us</h2>
                            <img src="/images/icon-mail.svg" alt="">
                        </div>
                        <p>
                            <a href="mailto:contact@fomaxdigitalsolution.com">
                                contact@fomaxdigitalsolution.com
                            </a>
